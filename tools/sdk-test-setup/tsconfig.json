{
  "extends": "../../node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "moduleResolution": "node",
    "outDir": "build",
    "forceConsistentCasingInFileNames": true,
