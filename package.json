{
  "name": "fimidara-server-node",
  "version": "0.1.0",
  "description": "fimidara file storage service server Node.js",
  "author": {
    "email": "ywordk@gmail.com",
    "name": "softkave"
  },
  "license": "MIT",
  "keywords": [],
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "engines": {
    "node": "21.x",
    "npm": "10.x"
  },
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "start": "node build/src/index.js",
    "pretest": "npm run compile",
    "prod": "tsc && env-cmd -f \".env.prod\" npm start",
    "dev": "tsc && env-cmd -f \".env.dev\" npm start",
    "test": "env-cmd -f \".env.unit-test\" vitest --config ./vitest.config.ts",
    "fn-test": "env-cmd -f \".env.unit-test\" vitest --config ./vitest.fn.config.ts",
    "integration-test": "env-cmd -f \".env.integration-test\" vitest --config ./vitest.config.ts",
    "dev-for-test": "tsc && env-cmd -f \".env.unit-test\" npm start",
    "check-circular-deps": "npx dpdm --no-warning --no-tree ./src/index.ts",
    "gen-rest-api": "env-cmd -f \".env.dev\" ts-node src/mddoc/restApiGen.ts",
    "gen-js-sdk": "env-cmd -f \".env.dev\" ts-node src/mddoc/jsSdkCodeGen.ts",
    "dev-mongo-rs": "npx run-rs --host localhost --portStart 27000 --dbpath dev-mongo-rs-data --keep",
    "test-mongo-rs": "npx run-rs --host localhost --portStart 27000 --dbpath test-mongo-rs-data",
    "render-email-templates": "env-cmd -f \".env.dev\" ts-node src/tools/email-templates/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.52.0",
    "@aws-sdk/client-secrets-manager": "^3.465.0",
    "@aws-sdk/client-sesv2": "^3.52.0",
    "@aws-sdk/lib-storage": "^3.490.0",
    "argon2": "^0.40.1",
    "config": "^3.3.9",
    "connect-busboy": "^1.0.0",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "date-fns": "^2.16.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-jwt": "^8.3.0",
    "fs-extra": "^11.1.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.20",
    "mongoose": "^8.1.2",
    "nanoid": "^5.0.7",
    "reflect-metadata": "^0.2.1",
    "safe-stable-stringify": "^2.3.1",
    "sharp": "^0.32.6",
    "softkave-js-utils": "^0.4.2",
    "stripe": "^8.222.0",
    "tsyringe": "^4.8.0",
    "type-fest": "^4.17.0",
    "validator": "^13.5.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/config": "^3.3.3",
    "@types/connect-busboy": "^1.0.0",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/lodash": "^4.14.165",
    "@types/multer": "^1.4.5",
    "@types/node": "^20.10.7",
    "@types/sharp": "^0.26.1",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "env-cmd": "^10.1.0",
    "expect-type": "^0.19.0",
    "gts": "^5.2.0",
    "inquirer": "^9.2.15",
    "kerberos": "^2.1.0",
    "run-rs": "^0.7.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.5.3"
  }
}
