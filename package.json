{
  "name": "fimidara-server-node",
  "version": "0.1.0",
  "description": "Fimidara file storage service server Node.js",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "license": "Apache-2.0",
  "keywords": [],
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "start": "node build/src/index.js",
    "pretest": "npm run compile",
    "prod": "tsc && env-cmd -f \".env.prod\" npm start",
    "dev": "tsc && env-cmd -f \".env.dev\" npm start",
    "test": "env-cmd -f \".env.unit-test\" jest --config=jest.config.ts --detectOpenHandles",
    "integration-test": "env-cmd -f \".env.integration-test\" jest --config=jest.config.integration-test.ts --detectOpenHandles",
    "dev-for-test": "tsc && env-cmd -f \".env.unit-test\" npm start",
    "check-circular-deps": "npx dpdm --no-warning --no-tree ./src/index.ts",
    "gen-rest-api": "env-cmd -f \".env.dev\" ts-node src/mddoc/restApiGen.ts",
    "gen-js-sdk": "env-cmd -f \".env.dev\" ts-node src/mddoc/jsSdkCodeGen.ts",
    "dev-mongo-rs": "npx run-rs --host localhost --portStart 27000 --dbpath dev-mongo-rs-data",
    "test-mongo-rs": "npx run-rs --host localhost --portStart 27000 --dbpath test-mongo-rs-data",
    "render-email-templates": "env-cmd -f \".env.dev\" ts-node src/tools/email-templates/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.52.0",
    "@aws-sdk/client-secrets-manager": "^3.465.0",
    "@aws-sdk/client-sesv2": "^3.52.0",
    "argon2": "^0.27.1",
    "connect-busboy": "^1.0.0",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "date-fns": "^2.16.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-jwt": "^8.3.0",
    "fs-extra": "^11.1.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.20",
    "mongoose": "^6.8.3",
    "nanoid": "^3.1.20",
    "safe-stable-stringify": "^2.3.1",
    "sharp": "^0.32.6",
    "stripe": "^8.222.0",
    "tsyringe": "^4.8.0",
    "type-fest": "^3.8.0",
    "validator": "^13.5.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/connect-busboy": "^1.0.0",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.165",
    "@types/multer": "^1.4.5",
    "@types/node": "^16.18.11",
    "@types/sharp": "^0.26.1",
    "@types/validator": "^13.1.2",
    "env-cmd": "^10.1.0",
    "gts": "^3.1.0",
    "inquirer": "^8.0.0",
    "jest": "^28.1.3",
    "run-rs": "^0.7.7",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
