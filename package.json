{
  "name": "fimidara-server-node",
  "version": "0.0.0",
  "description": "Fimidara file storage service server Node.js",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "start": "node build/src/index.js",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "render-email-templates": "tsc --project email-templates/tsconfig.json && node email-templates/build/email-templates/index.js",
    "test": "env-cmd -f \".env.test\" jest --config=jest.config.test.ts --detectOpenHandles",
    "integration-test": "env-cmd -f \".env.integration-test\" jest --config=jest.config.integration-test.ts --detectOpenHandles",
    "dev": "tsc && env-cmd -f \".env\" npm start",
    "mongo-local-dev": "tsc && env-cmd -f \".env.local\" npm start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.52.0",
    "@aws-sdk/client-sesv2": "^3.52.0",
    "argon2": "^0.27.1",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-jwt": "^6.0.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^6.2.2",
    "multer": "^1.4.4",
    "nanoid": "^3.1.20",
    "sharp": "^0.27.0",
    "stripe": "^8.222.0",
    "validator": "^13.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/express-jwt": "^6.0.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/multer": "^1.4.5",
    "@types/node": "^16.11.36",
    "@types/sharp": "^0.26.1",
    "@types/validator": "^13.1.2",
    "env-cmd": "^10.1.0",
    "gts": "^3.1.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
